<connectors>
	<connector>
		<provider>Oracle</provider>
		<mnemonic>ORA</mnemonic>
		<jdbc_driver_class>oracle.jdbc.driver.OracleDriver</jdbc_driver_class>
		<base_url>jdbc:oracle:thin:@</base_url>
		<port_seperator>:</port_seperator>
		<db_seperator>:</db_seperator>
	</connector>
	<connector>
		<provider>MSSql</provider>
		<mnemonic>MSSql</mnemonic>
		<jdbc_driver_class>com.microsoft.sqlserver.jdbc.SQLServerDriver</jdbc_driver_class>
		<base_url>jdbc:sqlserver://</base_url>
		<port_seperator>:</port_seperator>
		<db_seperator></db_seperator>
	</connector>
	<connector>
		<provider>MySql</provider>
		<mnemonic>MySql</mnemonic>
		<jdbc_driver_class>com.mysql.jdbc.Driver</jdbc_driver_class>
		<base_url>jdbc:mysql://</base_url>
		<port_seperator>:</port_seperator>
		<db_seperator>/</db_seperator>
	</connector>
	<connector>
		<provider>DB2</provider>
		<mnemonic>DB2</mnemonic>
		<jdbc_driver_class>COM.ibm.db2.jdbc.net.DB2Driver</jdbc_driver_class>
		<base_url>jdbc:db2:</base_url>
		<port_seperator>:</port_seperator>
		<db_seperator>/</db_seperator>
	</connector>
	<connector>
		<provider>Sybase</provider>
		<mnemonic>Sybase</mnemonic>
		<jdbc_driver_class>com.sybase.jdbc.SybDriver</jdbc_driver_class>
		<base_url>jdbc:sybase:Tds:</base_url>
		<port_seperator>:</port_seperator>
		<db_seperator>/</db_seperator>
	</connector>
	<connector>
		<provider>Excel</provider>
		<mnemonic>Excel</mnemonic>
		<jdbc_driver_class>sun.jdbc.odbc.JdbcOdbcDriver</jdbc_driver_class>
		<base_url>jdbc:odbc:Driver={Microsoft Excel Driver (*.xls)};DBQ=</base_url>
		<port_seperator></port_seperator>
		<db_seperator></db_seperator>
	</connector>
</connectors>